{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "shop-api-route": {
        "ClusterId": "shop-cluster",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "shop-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:5081"
          }
        }
      }
    }
  }
}
