{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "docs/v1",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5203"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "docs/v1",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:5081;http://localhost:5080"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      //"launchBrowser": true,
      "launchUrl": "docs/v1",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/docs/v1",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "5081",
        "ASPNETCORE_HTTP_PORTS": "5080"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:19152",
      "sslPort": 44340
    }
  }
}